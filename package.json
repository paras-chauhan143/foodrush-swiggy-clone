{
  "name": "swiggy-clone",
  "version": "1.0.0",
  "description": "Swiggy clone using React + Parcel",
  "main": "index.js",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url ./ && npm run postbuild",
    "postbuild": "cp dist/index.html dist/404.html",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "parcel": "^2.15.4",
    "gh-pages": "^6.1.1"
  }
}
